//Implement a simple "password" system that takes a password in the form of a number.  Make it so that either of two numbers is valid, but use only one if statement to do the check.


#include <iostream>

using namespace std;

int main()
{
    int password;
    int password_number = 95;

    cout << "Please enter the password number" << endl;
    cin >> password;

    if ( password == password_number)
    {
        cout << "Access Granted!!" << endl;
    }
    else
    {
        cout << "Access, Denied!" << endl;
    }

    return 0;
}
